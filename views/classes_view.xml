<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="discipline_classes_model_view_tree" model="ir.ui.view">
            <field name="name">discipline_classes_model_view_tree</field>
            <field name="model">classes.model</field>
            <field name="arch" type="xml">
                <tree string="CLASSES" editable="top">
                    <field name="name" widget="char_emojis"/>
                    <field name="code"/>
                    <field name="major_id"/>
                    <field name="teacher_id" widget="many2one_avatar_user"/>
                </tree>
            </field>
        </record>


    </data>
</odoo>